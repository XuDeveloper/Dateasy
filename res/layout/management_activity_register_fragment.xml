<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eeeeee" >
	<TextView 
	    android:id="@+id/management_activity_register_count"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="23"
	    android:textSize="12sp"
	    android:textColor="#666666"
	    android:layout_alignParentLeft="true"
	    android:layout_alignParentTop="true"
	    android:layout_marginLeft="16dp"
	    android:layout_marginTop="8dp"/>
	<TextView 
	    android:id="@+id/management_activity_register_tv1"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="人报名，"
	    android:textColor="#666666"
	    android:textSize="12sp"
	    android:layout_toRightOf="@id/management_activity_register_count"
	    android:layout_alignTop="@id/management_activity_register_count"/>
	<TextView 
	    android:id="@+id/management_activity_register_checkcount"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="1"
	    android:textSize="12sp"
	    android:textColor="#666666"
		android:layout_toRightOf="@id/management_activity_register_tv1"
		android:layout_alignTop="@id/management_activity_register_tv1"/>
	<TextView 
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:text="人验票"
	    android:textColor="#666666"
	    android:textSize="12sp"
	    android:layout_toRightOf="@id/management_activity_register_checkcount"
	    android:layout_alignTop="@id/management_activity_register_checkcount"/>
	<ListView 
	    android:id="@+id/management_activity_register_lv"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_marginLeft="16dp"
	    android:layout_marginRight="16dp"
	    android:layout_marginTop="24dp"
	    android:background="#ffffff"></ListView>

</RelativeLayout>
